{
    "manifest_version": 3,
    "name": "Snow panda",
    "version": "1.0",
    "description": "Life easier on websites",
    "permissions": ["declarativeNetRequest", "storage"],
    "host_permissions": ["<all_urls>"],
    "icons": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "declarative_net_request": {
      "rule_resources": [
        {
          "id": "block_rules",
          "enabled": true,
          "path": "rules.json"
        }
      ]
    }
  }
  